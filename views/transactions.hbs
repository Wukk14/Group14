</head>

<body>
<header>
    <!--NavBar-->
    <nav class="navbar navbar-expand-md navbar-light bg-light fixed-top">
        <div class="container">
            <!--Logo-->
            <a class="navbar-brand" href="home"><img src="/static/logo.png" alt="logo"></a>

            <!--Burger Button-->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>

            <!--Main Navbar list-->
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active"><a class="nav-link hvr-underline-from-center" href="home">HOME<span class="sr-only">(current)</span></a></li>
                    <li class="nav-item active"><a class="nav-link hvr-underline-from-center" href="trading-success">TRADE</a></li>
                    <div class="dropdown">
                        <!-- <button class="dropbtn"> -->
                        <li class="nav-item active"><a class="nav-link hvr-underline-from-center dropbtn">ACCOUNT</a></li>
                        <!-- </button> -->
                        <div class="dropdown-content">
                        <a href="transactions">Transactions</a>
                        <a href="profile">Profile</a>
                        {{#if admin}}
                        <a href="admin-success">Admin Panel</a>
                        {{/if}}
                        </div>
                    </div>
                    <div class="mycontent-left" style="border-right: 1px solid #333;"></div>
                    <!--Logout-->
                    <a class="navbar-brand logout" style="padding: 2px 0px 0px 10px;" href="login"><img src="/static/logout.png" alt="logout"></a>
                </ul>
            </div>
        </div>
    </nav>
    <!-- End of Navbar -->

    <!-- Stock Ticker -->
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" type="text/javascript"></script>
    <script src="/static/scripts.js" type="text/javascript"></script>
    <script>
        var gainTicker = new WBIHorizontalTicker('gainers');
        gainTicker.start();

    </script>
</header>

<!-- Page content -->
<div class="container main" style="background-color: #DDDDDD; height: 80vh; box-shadow: 0 4px 500px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);">
    <div class="container-fluid" style="padding-top: 0px;">
        <h3 style="padding-bottom: 5px;"><span style="font-weight: lighter;" id="date"></span></h3>


            <div class='container form-style-8'>
            

                    <h2>TRANSACTIONS</h2>

                    <table name="Transactions" id="sideTables">
                    <tr>

                    <th>Time of Transaction</th>
                    <th>Action</th>
                    <th>Ticker</th>
                    <th>Company</th>
                    <th>Quantity</th>
                    <th>Cost/Share ($)</th>
                    <th>Total Price ($)</th>
                    <th>New Balance ($)</th>
                    </tr>
                    <p>{{#each transactions}}
                    <tr>
                        <td style="">{{this.date}}. </td>
                        <td style="">{{this.action_type}}</td>
                        <td style="">{{this.symbol}}</td>
                        <td style="">{{this.company}}</td>
                        <td style="">{{this.qty}}</td>
                        <td style="">{{this.cost_per_share}}</td>
                        <td style=""><span style="font-weight: bold;"></span><div id="user_cash">{{this.total}}</div></td>
                        <td style="">{{this.balance_after_transaction}}</td>
                    </tr>
                    {{/each}}</p>
                    </table>
                    

                <br><br>

        
                
            </div>
        
        <br>

    </div>
    <br>
    <br>
    <br>
    <hr>
    </div>
